    {
      "localRoot": "${workspaceFolder}",
      "name": "Attach to Docker",
      "port": 9222,
      "remoteRoot": "/app",
      "request": "attach",
      "restart": true,
      "timeout": 60000,
      "type": "node"
    }
  ]
